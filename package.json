{
  "name": "@bitblit/ports-of-call",
  "version": "0.3.0",
  "description": "Serial device detector and wrapper",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/ports-of-call.js",
    "dist/ports-of-call.d.ts",
    "dist/devices",
    "dist/model"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run compile",
    "clean-compile": "yarn run clean && yarn run compile",
    "compile": "tsc",
    "watch": "tsc -w .",
    "clean": "shx rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "apply-circle": "node node_modules/@bitblit/ratchet/dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json",
    "cli": "yarn run compile && node dist/cli",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/PortsOfCall"
  },
  "keywords": [
    "serial",
    "port",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/PortsOfCall/issues"
  },
  "homepage": "https://github.com/bitblit/PortsOfCall#readme",
  "engines": {
    "node": ">=12.12"
  },
  "license": "Apache-2.0",
  "resolutions": {
    "ini": "^1.3.6"
  },
  "dependencies": {
    "@bitblit/ratchet": "0.11.4",
    "gps": "0.6.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.32",
    "rxjs": "^7.5.5",
    "serialport": "9.0.6"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "@types/mocha": "8.2.0",
    "@types/node": "12.12.6",
    "@types/serialport": "8.0.1",
    "chai": "4.3.0",
    "chai-http": "4.3.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.7",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "mocha": "8.2.1",
    "shx": "0.3.3",
    "ts-node": "9.1.1",
    "typescript": "3.9.4"
  }
}
