{
  "name": "@bitblit/ports-of-call",
  "version": "0.2.0",
  "description": "Serial device detector and wrapper",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/ports-of-call.js",
    "dist/ports-of-call.d.ts",
    "dist/devices",
    "dist/model"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>"
  ],
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run compile",
    "compile": "yarn run clean && tsc",
    "compile-win": "yarn run clean-win && tsc",
    "watch": "tsc -w .",
    "clean": "rm -Rf dist",
    "clean-win": "if exist dist rmdir /q /s dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "apply-circle": "node node_modules/@bitblit/ratchet/dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json",
    "cli": "yarn run compile && node dist/cli",
    "cli-win": "yarn run compile-win && node dist/cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/PortsOfCall"
  },
  "keywords": [
    "serial",
    "port",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/PortsOfCall/issues"
  },
  "homepage": "https://github.com/bitblit/PortsOfCall#readme",
  "engines": {
    "node": ">=10.12"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "0.3.5",
    "serialport": "7.1.5",
    "gps": "0.5.3",
    "rxjs": "6.5.2",
    "moment": "2.24.0",
    "moment-timezone": "0.5.26"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.30",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "mocha": "5.2.0",
    "ts-node": "7.0.1",
    "tslint": "5.18.0",
    "typescript": "3.1.2"
  }
}
